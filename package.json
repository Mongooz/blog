{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "start": "next dev --webpack",
    "dev": "cross-env INIT_CWD=$PWD next dev --webpack",
    "build": "cross-env INIT_CWD=$PWD next build --webpack && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start --webpack",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "lint": "next lint --webpack --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@next/bundle-analyzer": "16.1.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "next",
    "@vercel/analytics": "^1.6.1",
    "autoprefixer": "^10.4.23",
    "body-scroll-lock": "^4.0.0-beta.0",
    "contentlayer2": "0.5.8",
    "esbuild": "0.27.2",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "2.0.2",
    "lightgallery": "^2.9.0",
    "next": "16.1.4",
    "next-contentlayer2": "0.5.8",
    "next-themes": "^0.4.6",
    "pliny": "0.4.1",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-masonry-css": "^1.0.16",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.3.1",
    "rehype-katex": "^7.0.0",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "next",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdx": "^2.0.12",
    "@types/react": "19.2.8",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "latest",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1",
  "resolutions": {
    "@types/react": "19.2.8"
  }
}
